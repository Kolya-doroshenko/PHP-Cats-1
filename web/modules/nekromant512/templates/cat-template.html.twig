{{ form }}
{% for item in items %}
  <div class="row underline_cats">
    <div class="info">
      <p><span>Name: </span>{{ item.name }}</p>
      <p><span>Email: </span>{{ item.mail }}</p>
      <p><span>Submitted: </span>{{ item.created }}</p>
    </div>
    <div class="image">
      <a href="{{ item.images }}"><div class="image-container">{{ item.image }}</div></a>
    </div>
      {% if user.hasPermission('administer nodes') %}
    <div class="buttons">
      <div class="button">
        <a href="/admin/nekromant512/catsChange/{{ item.id }}">Edit </a>
      </div>
      <div class="button">
        <a href="/nekromant512/catsDel/{{ item.id }}" class="use-ajax" data-dialog-type="modal">Delete</a>
      </div>
    </div>
      {% endif %}
  </div>
{% endfor %}
